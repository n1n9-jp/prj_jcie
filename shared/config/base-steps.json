{
  "version": "2.0",
  "description": "Base step structure - common to all diseases. Disease-specific values should override via disease-specific config files.",
  "steps": [
    {
      "id": "opening",
      "text": {
        "content": "",
        "visible": false
      },
      "chart": {
        "visible": false
      },
      "map": {
        "visible": false
      },
      "image": {
        "src": "assets/images/opening.jpg",
        "alt": "{{ disease_title }}",
        "visible": true,
        "position": {
          "horizontal": "center",
          "vertical": "center",
          "width": "100%",
          "height": "100%"
        },
        "config": {
          "opacity": 1.0,
          "objectFit": "cover",
          "specialMode": "step0-background"
        }
      }
    },
    {
      "id": "intro",
      "text": {
        "content": "{{ intro_text }}",

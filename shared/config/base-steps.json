          "vertical": "center"
        }
      },
      "chart": {
        "visible": false
      },
      "map": {
        "visible": true,
        "center": [70, 10],
        "zoom": 1.8,
        "widthPercent": 100,
        "heightPercent": 100,
        "useRegionColors": true,
        "lightenAllCountries": true,
        "targetRegions": "{{ early_expansion_regions }}",
        "showSpreadingArrows": false
      },
      "image": {
        "visible": false
      }
    },
    {
      "id": "peak-crisis",
      "text": {
        "content": "{{ peak_crisis_title }}",
        "position": {
          "width": "30%",
          "horizontal": "right",
          "vertical": "center"
        }
      },
      "chart": {
        "type": "line",
        "dataFile": "{{ peak_crisis_data_file }}",
        "visible": true,
        "position": {
          "horizontal": "left",
          "vertical": "center",
          "width": "70%",
          "height": "100%",
          "padding": "2rem"
        },
        "config": {
          "widthPercent": 70,
          "heightPercent": 100,
          "xField": "year",
          "yField": "value",
          "xAxisLabel": "年",
          "yAxisLabel": "{{ peak_crisis_y_axis_label }}",
          "seriesField": "series",
          "multiSeries": true,
          "title": "{{ peak_crisis_chart_title }}",
          "dataSource": "Our World in Data",
          "legendType": "inline",
          "yAxisFormat": {
            "type": "japanese",
            "decimals": 0,
            "units": {
              "億": 100000000,
              "万": 10000
            }
          },
          "filter": "{{ peak_crisis_filter }}",
          "yRange": [0, 4000000],
          "animation": {
            "mode": "progressive",
            "duration": 3000,
            "stepDelay": 200
          },
          "annotations": "{{ peak_crisis_annotations }}"
        }
      },
      "map": {
        "visible": false
      },
      "image": {
        "visible": false
      }
    },
